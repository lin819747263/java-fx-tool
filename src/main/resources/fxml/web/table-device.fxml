<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>


<TableView editable="true"
           xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:id="table"
           fx:controller="com.kehua.xml.controller.web.TableDeviceController">
    <columns>
        <TableColumn prefWidth="75.0" text="序号" fx:id="num" />
        <TableColumn prefWidth="75.0" text="模板编码" fx:id="templateCode" />
        <TableColumn prefWidth="75.0" text="设备类型(*)" fx:id="devType" />
        <TableColumn prefWidth="75.0" text="语言(*)" fx:id="language" />
        <TableColumn prefWidth="75.0" text="协议名称(*)" fx:id="protocolName" />
        <TableColumn prefWidth="75.0" text="标识" fx:id="maker" />
        <TableColumn prefWidth="75.0" text="类型" fx:id="model" />
        <TableColumn prefWidth="75.0" text="描述" fx:id="describe" />
        <TableColumn prefWidth="75.0" text="协议类型" fx:id="protocolType" />
    </columns>
</TableView>

