<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<TableView editable="true"
           xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:id="table"
           fx:controller="com.kehua.xml.controller.web.TableEventController">
    <columns>
        <TableColumn prefWidth="75.0" text="序号" fx:id="num" />
        <TableColumn prefWidth="75.0" text="点位名称" fx:id="remark"/>
        <TableColumn prefWidth="75.0" text="事件等级" fx:id="eventLevel"/>
        <TableColumn prefWidth="75.0" text="处理建议" fx:id="recommend"/>
    </columns>
</TableView>
