<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<TableView editable="true"
           xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:id="table"
           fx:controller="com.kehua.xml.controller.web.TableI18nController">
    <columns>
        <TableColumn prefWidth="75.0" text="序号" fx:id="num" />
        <TableColumn prefWidth="75.0" text="点位名称" fx:id="remark" />
        <TableColumn prefWidth="75.0" text="英语" fx:id="remarkEn" />
        <TableColumn prefWidth="75.0" text="波兰语" fx:id="remarkPl" />
        <TableColumn prefWidth="75.0" text="越南语" fx:id="remarkVnm" />
        <TableColumn prefWidth="75.0" text="西班牙语" fx:id="remarkEs" />
    </columns>
</TableView>
